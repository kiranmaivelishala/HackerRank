class MyBook extends Book{
    int price;
    public MyBook(String btitle,String bauthor, int price1)
    {
      super(btitle,bauthor);
        this.price=price1;
    }
void display()
    {
        System.out.println("Title: "+title);
    System.out.println("Author: "+author);
    System.out.println("Price: "+price);
    }
}
